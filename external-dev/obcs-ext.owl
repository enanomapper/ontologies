        <rdfs:label>exponential distribution</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBCS_0000210 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBCS_0000210">
